{
  "name": "ai-bridge",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "postinstall": "npm run bootstrap",
    "start": "DUMI_THEME=dumi-theme-mobile dumi dev",
    "build": "father-build",
    "build-doc": "dumi build",
    "lint": "prettier packages/**/*.ts --check",
    "format": "prettier packages/**/*.ts --write",
    "ci": "npm run lint && npm run build",
    "test": "umi-test --coverage",
    "bootstrap": "lerna bootstrap --hoist",
    "prepub": "npm run ci",
    "pub": "lerna publish --force-publish=*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-version": "^0.2.3",
    "dumi": "^1.1.17",
    "dumi-theme-mobile": "1.0.16",
    "father-build": "^1.19.4",
    "gh-pages": "^3.0.0",
    "husky": "^6.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "umi-test": "^1.9.6"
  },
  "dependencies": {
    "antd-mobile": "^2.3.4"
  }
}
